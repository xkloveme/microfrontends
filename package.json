{
  "name": "qiankun",
  "version": "1.2.2",
  "description": "An completed implementation of Micro Frontends",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "concurrently \"npm run start:main\" \"npm run start:react16\" \"npm run start:react15\" \"npm run start:vue\"",
    "install:service": "npm run build && cd service/main && npm i && cd ../react16 && npm i && cd ../react15 && npm i && cd ../vue && npm i",
    "setup": "lerna link && lerna bootstrap",
    "dev": "lerna run dev --stream",
    "start:main": "cd service/main && npm start",
    "start:react16": "cd service/react16 && npm start",
    "start:react15": "cd service/react15 && npm start",
    "start:vue": "cd service/vue && npm start",
    "build": "father-build",
    "release": "np --no-cleanup --yolo --no-publish",
    "prepublishOnly": "npm run lint && npm run build",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:prettier": "check-prettier lint",
    "prettier": "prettier -c --write **/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuitos/qiankun.git"
  },
  "files": [
    "dist"
  ],
  "author": "Kuitos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuitos/qiankun/issues"
  },
  "homepage": "https://github.com/kuitos/qiankun#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "import-html-entry": "^1.2.6",
    "lodash": "^4.17.15",
    "single-spa": "^4.4.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.144",
    "@umijs/fabric": "^1.2.1",
    "babel-plugin-import": "^1.12.2",
    "check-prettier": "^1.0.3",
    "concurrently": "^5.0.0",
    "father-build": "^1.13.2",
    "husky": "^3.0.9",
    "lerna": "^3.13.0",
    "lint-staged": "^9.4.2",
    "np": "^5.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,json,css,md}": [
      "npm run prettier",
      "git add"
    ],
    "**/*.{js,ts}": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
